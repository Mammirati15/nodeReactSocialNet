c/App.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/index.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/MainRouter.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/core/Home.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/Signup.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/Signin.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/core/Menu.js",["88"],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/auth/index.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/Profile.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/apiUser.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/Users.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/DeleteUser.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/EditProfile.js",["89","90"],"import React, { Component } from 'react'\nimport {isAuthenticated} from '../auth/index'\nimport {read, update, updateUser} from \"./apiUser\"\nimport {Redirect} from 'react-router-dom'\nimport DefaultProfile from '../images/avatar.jpg'\n\n\nclass EditProfile extends Component {\n  constructor(){\n    super();\n    this.state = {\n      id: \"\",\n      name: \"\",\n      email: \"\",\n      about: \"\",\n      password: \"\",\n      redirectToProfile: false,\n      error: \"\",\n      fileSize: 0,\n      loading: false\n    }\n  }\n\n  init = userId => {\n    const token = isAuthenticated().token \n    read(userId, token)\n     .then(data => {\n       if(data.error){\n         this.setState({redirectToProfile: true})\n       } else {\n         this.setState({id: data._id, name: data.name, email: data.email, error: \"\", about: data.about})\n       }\n     })\n   }\n   \n  componentDidMount() {\n    this.userData = new FormData()\n    const userId = this.props.match.params.userId\n    this.init(userId)\n  }\n\n  isValid = () => {\n    const {name, email, password, fileSize} = this.state\n    if(fileSize > 100000) {\n      this.setState({error: \"File Size must be smaller than 100KB\", loading: false})\n      return false\n    }\n    \n    if(name.length === 0) {\n      this.setState({error: \"Name is required\", loading: false})\n      return false\n    }\n    if(!/^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$/.test(email)) {\n      this.setState({error: \"Must Use a Valid Email Address\", loading: false})\n      return false\n    }\n    if(password.length >= 1 && password.length <= 5) {\n      this.setState({error: \"Password must be at least 6 characters long\", loading: false})\n      return false\n    }\n    return true\n  }\n\n  handleChange = (name) => (event) => {\n    this.setState({error: \"\"})\n    const value = name === 'photo' ? event.target.files[0] : event.target.value\n\n    const fileSize = name === 'photo' ? event.target.files[0].size : 0\n    this.userData.set(name, value)\n    this.setState({[name]: value, fileSize})\n  }\n\n  clickSubmit = event => {\n    event.preventDefault()\n    this.setState({loading: true})\n\n    if(this.isValid()) {\n      \n    \n      //console.log(user)\n      const userId = this.props.match.params.userId\n      const token = isAuthenticated().token \n\n      update(userId, token, this.userData)\n      .then(data => {\n        if(data.error) this.setState({error: data.error})\n          else\n            updateUser(data, () => {\n              this.setState({\n                redirectToProfile: true\n            })\n          })             \n      })\n    } \n  }\n\n  signUpForm = (name, email, about, password) => (\n    <form className=\"ml-5\">\n\n    <div className=\"form-group\">\n      <label className=\"text-muted\">Profile Photo</label>\n      <input onChange={this.handleChange(\"photo\")} type=\"file\" accept=\"image/*\" className=\"form-control\" />\n    </div>\n\n\n    <div className=\"form-group\">\n      <label className=\"text-muted\">Name</label>\n      <input onChange={this.handleChange(\"name\")} type=\"text\" className=\"form-control\" value={name} />\n    </div>\n    <div className=\"form-group\">\n      <label className=\"text-muted\">Email</label>\n      <input onChange={this.handleChange(\"email\")} type=\"email\" className=\"form-control\" value={email} />\n    </div>\n    <div className=\"form-group\">\n      <label className=\"text-muted\">About</label>\n      <textarea onChange={this.handleChange(\"about\")} type=\"text\" className=\"form-control\" value={about} />\n    </div>\n    <div className=\"form-group\">\n      <label className=\"text-muted\">Password</label>\n      <input onChange={this.handleChange(\"password\")} type=\"password\" className=\"form-control\" value={password} />\n    </div>\n    <button onClick={this.clickSubmit} className=\"mt-2 btn btn-raised btn-primary\">Update</button>\n  </form>\n  )\n\n  render() {\n    const {id, name, email, password, redirectToProfile, error, loading, about} = this.state\n\n    if(redirectToProfile) {\n      return <Redirect to={`/user/${id}`} />\n    }\n\n    const photoUrl = id ? `${process.env.REACT_APP_API_URL}/user/photo/${id}?${new Date().getTime()}` : DefaultProfile\n\n    return (\n      <div className=\"container\">\n        <h2 className=\"mt-5 mb-5 ml-5\">Edit Profile</h2>\n\n        <div className=\"alert alert-danger\" style={{ display: error ? \"\" : \"none\" }}>\n          {error}\n        </div>\n\n        {loading ? <div className=\"jumbotron text-center\"><h2>Loading...</h2></div> : \"\"}\n\n        <img style={{height: \"200px\", width: \"auto\"}} \n          className=\"ml-5 img-thumbnail\" \n          src={photoUrl} \n          onError={i => (i.target.src = `${DefaultProfile}`)} \n          alt={name} \n        />\n\n        {this.signUpForm(name, email, about, password)}\n\n      </div>\n    )\n  }\n}\n\nexport default EditProfile","/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/auth/PrivateRoute.js",["91"],"import React, {Component} from 'react'\nimport {Route, Redirect} from 'react-router-dom'\nimport {isAuthenticated} from './index'\n\nconst PrivateRoute = ({component: Component, ...rest}) => (\n  //props means components passed down to this private route component\n  <Route {...rest} render={props => isAuthenticated() ? (\n    <Component {...props} />\n  ) : (\n    <Redirect to={{pathname: \"/signin\", state: {from: props.location}}} />\n  )} />\n)\n\nexport default PrivateRoute","/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/FollowProfileButton.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/ProfileTabs.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/FindPeople.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/post/NewPost.js",["92","93"],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/post/apiPost.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/post/Posts.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/post/SinglePost.js",["94"],{"ruleId":"95","severity":1,"message":"96","line":55,"column":75,"nodeType":"97","messageId":"98","endLine":55,"endColumn":76},{"ruleId":"99","severity":1,"message":"100","line":53,"column":16,"nodeType":"101","messageId":"102","endLine":53,"endColumn":17,"suggestions":"103"},{"ruleId":"99","severity":1,"message":"100","line":53,"column":32,"nodeType":"101","messageId":"102","endLine":53,"endColumn":33,"suggestions":"104"},{"ruleId":"105","severity":1,"message":"106","line":1,"column":16,"nodeType":"107","messageId":"108","endLine":1,"endColumn":25},{"ruleId":"105","severity":1,"message":"109",