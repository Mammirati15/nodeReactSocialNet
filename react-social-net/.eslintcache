\nimport {isAuthenticated} from '../auth/index'\nimport {read} from \"./apiUser\"\n\nclass Profile extends Component {\n  constructor(){\n    super()\n    this.state = {\n      user: \"\",\n      redirectToSignin: false\n    }\n  }\n\n  \n\n  init = (userId) => {\n   const token = isAuthenticated().token \n   read(userId, token)\n    .then(data => {\n      if(data.error){\n        this.setState({redirectToSignin: true})\n      } else {\n        this.setState({user: data})\n      }\n    })\n  }\n  \n  componentDidMount() {\n    const userId = this.props.match.params.userId\n    this.init(userId)\n  }\n\n  render() {\n    const {redirectToSignin, user} = this.state\n    if (redirectToSignin) return <Redirect to=\"/signin\" />\n    \n    return (\n      <div className=\"container\">\n        <div className=\"row\">\n          <div className=\"col-md-6\">\n            <h2 className=\"mt-5 mb-5\">Profile</h2>\n            <p>Hello {isAuthenticated().user.name}</p>\n            <p>Email: {isAuthenticated().user.email}</p>\n            <p>{`Joined: ${new Date(user.created).toDateString()}`}</p>\n          </div>\n          <div className=\"col-md-6\">\n            {isAuthenticated().user && isAuthenticated().user._id == user._id &&(\n              <div className=\"d-inline-block mt-5\">\n                <Link className=\"btn btn-raised btn-success mr-5\" to={`/user/edit/${user._id}`}>\n                  Edit Profile\n                </Link>\n                <button className=\"btn btn-raised btn-danger\">\n                  Delete Profile\n                </button>\n              </div>\n            )}\n          </div>\n        </div>\n      </div>\n    )\n  }\n}\n\nexport default Profile","/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/apiUser.js",[],"/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/user/Users.js",["50"],{"ruleId":"51","severity":1,"message":"52","line":35,"column":11,"nodeType":"53","endLine":35,"en