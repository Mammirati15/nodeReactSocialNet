 const userId = this.props.match.params.userId\n    this.init(userId)\n  }\n\n  isValid = () => {\n    const {name, email, password} = this.state\n    if(name.length === 0) {\n      this.setState({error: \"Name is required\"})\n      return false\n    }\n    if(!/^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$/.test(email)) {\n      this.setState({error: \"Must Use a Valid Email Address\"})\n      return false\n    }\n    if(password.length >= 1 && password.length <= 5) {\n      this.setState({error: \"Password must be at least 6 characters long\"})\n      return false\n    }\n    return true\n  }\n\n  handleChange = (name) => (event) => {\n    const value = name === 'photo' ? event.target.files[0] : event.target.value\n    this.userData.set(name, value)\n    this.setState({[name]: value})\n  }\n\n  clickSubmit = event => {\n    event.preventDefault()\n    this.setState({loading: true})\n\n    if(this.isValid()) {\n      \n    \n      //console.log(user)\n      const userId = this.props.match.params.userId\n      const token = isAuthenticated().token \n\n      update(userId, token, this.userData)\n      .then(data => {\n        if(data.error) this.setState({error: data.error})\n          else this.setState({\n            redirectToProfile: true\n          })\n      })\n    } \n  }\n\n  signUpForm = (name, email, password) => (\n    <form className=\"ml-5\">\n\n    <div className=\"form-group\">\n      <label className=\"text-muted\">Profile Photo</label>\n      <input onChange={this.handleChange(\"photo\")} type=\"file\" accept=\"image/*\" className=\"form-control\" />\n    </div>\n\n\n    <div className=\"form-group\">\n      <label className=\"text-muted\">Name</label>\n      <input onChange={this.handleChange(\"name\")} type=\"text\" className=\"form-control\" value={name} />\n    </div>\n    <div className=\"form-group\">\n      <label className=\"text-muted\">Email</label>\n      <input onChange={this.handleChange(\"email\")} type=\"email\" className=\"form-control\" value={email} />\n    </div>\n    <div className=\"form-group\">\n      <label className=\"text-muted\">Password</label>\n      <input onChange={this.handleChange(\"password\")} type=\"password\" className=\"form-control\" value={password} />\n    </div>\n    <button onClick={this.clickSubmit} className=\"mt-2 btn btn-raised btn-primary\">Update</button>\n  </form>\n  )\n\n  render() {\n    const {id, name, email, password, redirectToProfile, error, loading} = this.state\n\n    if(redirectToProfile) {\n      return <Redirect to={`/user/${id}`} />\n    }\n\n    \n    return (\n      <div className=\"container\">\n        <h2 className=\"mt-5 mb-5 ml-5\">Edit Profile</h2>\n\n        <div className=\"alert alert-danger\" style={{ display: error ? \"\" : \"none\" }}>\n          {error}\n        </div>\n\n        {loading ? <div className=\"jumbotron text-center\"><h2>Loading...</h2></div> : \"\"}\n\n        {this.signUpForm(name, email, password)}\n\n      </div>\n    )\n  }\n}\n\nexport default EditProfile","/Users/mattammirati/Documents/node_react/node_proj5/react-social-net/src/auth/PrivateRoute.js",["63"],{"ruleId":"64","severity":1,"message":"65","line":35,"column":75,"nodeType":"66","messageId":"67","endLine":35,"endColumn":76},{"ruleId":"68","severity":1,"message":"69","line":60,"column":67,"nodeType":"70","messageId":"71","endLine":60,"endColumn":69},{"ruleId":"72","severity":1,"message":"73","line":45,"column":16,"nodeType":"74","messageId":"75","endLine":45,"endColumn":17,"suggestions":"76"},{"ruleId":"72","severity":1,"message":"73","line":45,"column":32,"nodeType":"74","messageId":"75","endLine":45,"endColumn":33,"suggestions":"77"},{"ruleId":"78","severity":1,"message":"79","line":1,"column":16,"nodeType":"80","messageId":"81","endLine":1,"endColumn":25},"no-sequences","Unexpected use of comma operator.","SequenceExpression","unexpectedCommaExpression","eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","no-useless-e